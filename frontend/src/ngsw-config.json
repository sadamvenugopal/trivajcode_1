{
    "index": "/index.html",
    "assetGroups": [
      {
        "name": "app",
        "installMode": "prefetch",
        "resources": {
          "files": [
            "/favicon.ico",
            "/index.html"
          ],
          "versionedFiles": [
            "/*.bundle.css",
            "/*.bundle.js",
            "/*.chunk.js"
          ]
        }
      },
      {
        "name": "assets",
        "installMode": "lazy",
        "updateMode": "prefetch",
        "resources": {
          "files": [
            "/assets/**"
          ]
        }
      }
    ],
    "dataGroups": [
      {
        "name": "api-performance",
        "urls": ["/api/**"],
        "cacheConfig": {
          "maxAge": "3d",
          "maxSize": 100,
          "strategy": "performance"
        }
      }
    ]
  }