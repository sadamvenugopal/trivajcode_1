  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "module": "es2020",
    "target": "es2022",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "types": []
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
